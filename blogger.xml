<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:css='false' b:js='false' b:version='2' expr:dir='data:blog.languageDirection' expr:lang='data:blog.locale'
  xmlns='http://www.w3.org/1999/xhtml'
  xmlns:b='http://www.google.com/2005/gml/b'
  xmlns:data='http://www.google.com/2005/gml/data'
  xmlns:expr='http://www.google.com/2005/gml/expr'>
  <head>
    <meta charset='utf-8'/>
    <meta content='width=device-width,initial-scale=1.0,minimum-scale=1.0' name='viewport'/>
    <b:include data='blog' name='all-head-content'/>
    <title>
      <data:blog.pageTitle/>
    </title>
    <b:skin><![CDATA[.quickedit{display:none;}]]></b:skin>
    <b:template-skin/>
    <b:include data='blog' name='google-analytics'/>
    <!--css begin-->
    <link href='https://cdn.jsdelivr.net/npm/uikit@3.5.10/dist/css/uikit.min.css' rel='stylesheet'/>
    <!--css end-->
  </head>
  <body>
    <b:section id='tonayu'/>
    <b:if cond='data:blog.pageType == &quot;index&quot;'>
      <div itemscope='itemscope' itemtype='http://schema.org/Blog' style='display: none;'>
        <meta expr:content='data:blog.title' itemprop='name'/>
        <b:if cond='data:blog.metaDescription'>
          <meta expr:content='data:blog.metaDescription' itemprop='description'/>
        </b:if>
      </div>
    </b:if>
    <div class='wrap'>
      <header>
        <nav class='uk-navbar uk-navbar-container' uk-navbar=''>
          <div class='uk-navbar-left'>
            <a class='uk-navbar-item uk-logo' expr:href='data:blog.homepageUrl'>
              <img class='uk-margin-small-right' src='https://lh3.googleusercontent.com/8VNaKqs9G7KM2zD2U9pphFryMNHB-El7EvCl0cACrposVIHvkko3ccViXkX8=s32'/>
              <data:blog.title/>
            </a>
          </div>
          <div class='uk-navbar-right'>
            <b:section id='navbar-menu' maxwidgets='1' name='navbar-menu' showaddelement='yes'>
              <b:widget id='PageList1' locked='false' title='Pages' type='PageList'>
                <b:widget-settings>
                  <b:widget-setting name='pageListJson'><![CDATA[{'home': {'href': 'https://blog.tonayu.com/', 'title': 'Home', 'position': 0}}]]></b:widget-setting>
                  <b:widget-setting name='homeTitle'>Home</b:widget-setting>
                </b:widget-settings>
                <b:includable id='main'>
                  <ul class='uk-navbar-nav uk-visible@m'>
                    <b:loop values='data:links' var='link'>
                      <li>
                        <b:class cond='data:link.isCurrentPage' name='uk-active'/>
                        <a expr:href='data:link.href'>
                          <data:link.title/>
                        </a>
                      </li>
                    </b:loop>
                  </ul>
                </b:includable>
              </b:widget>
            </b:section>
            <a class='uk-navbar-toggle uk-hidden@m' href='#' uk-navbar-toggle-icon='' uk-toggle='target: #toggle-nav'/>
          </div>
        </nav>
        <b:section id='navbar-menu2' maxwidgets='1' name='navbar-menu2' showaddelement='yes'>
          <b:widget id='PageList2' locked='false' title='Pages' type='PageList'>
            <b:widget-settings>
              <b:widget-setting name='pageListJson'><![CDATA[{'home': {'href': 'https://blog.tonayu.com/', 'title': 'Home', 'position': 0}}]]></b:widget-setting>
              <b:widget-setting name='homeTitle'>Home</b:widget-setting>
            </b:widget-settings>
            <b:includable id='main'>
              <div class='uk-background-muted uk-padding-small' hidden='' id='toggle-nav'>
                <ul class='uk-nav uk-nav-default'>
                  <b:loop values='data:links' var='link'>
                    <li>
                      <b:class cond='data:link.isCurrentPage' name='uk-active'/>
                      <a expr:href='data:link.href'>
                        <data:link.title/>
                      </a>
                    </li>
                  </b:loop>
                </ul>
              </div>
            </b:includable>
          </b:widget>
        </b:section>
      </header>
      <section>
        <div class='uk-padding-small'>
 main body
        </div>
      </section>
      <footer>
        <div class='uk-tile uk-tile-secondary uk-padding-small'>
          <div class='uk-grid-match uk-child-width-1-3@m' uk-grid=''>
            <div>
              <b:section id='footer-1-2' class='uk-card uk-card-body uk-padding-remove'>
                <b:widget id='PopularPosts1' locked='false' title='Popular Posts' type='PopularPosts' version='1'>
                  <b:widget-settings>
                    <b:widget-setting name='numItemsToShow'>1</b:widget-setting>
                    <b:widget-setting name='showThumbnails'>false</b:widget-setting>
                    <b:widget-setting name='showSnippets'>false</b:widget-setting>
                    <b:widget-setting name='timeRange'>ALL_TIME</b:widget-setting>
                  </b:widget-settings>
                  <b:includable id='main'>
                    <b:if cond='data:title != &quot;&quot;'>
                      <h3 class='uk-card-title'>
                        <data:title/>
                      </h3>
                    </b:if>
                    <dl>
                      <b:loop values='data:posts' var='post'>
                        <dt>
                          <a expr:href='data:post.href'>
                            <data:post.title/>
                          </a>
                        </dt>
                        <dd>
                          <data:post.snippet/>
                        </dd>
                      </b:loop>
                    </dl>
                  </b:includable>
                </b:widget>
              </b:section>
            </div>
            <div>
              <b:section id='footer-2-2' class='uk-card uk-card-body uk-padding-remove'>
                <b:widget id='FeaturedPost1' locked='false' title='Featured Post' type='FeaturedPost'>
                  <b:widget-settings>
                    <b:widget-setting name='showSnippet'>false</b:widget-setting>
                    <b:widget-setting name='showPostTitle'>true</b:widget-setting>
                    <b:widget-setting name='showFirstImage'>false</b:widget-setting>
                    <b:widget-setting name='useMostRecentPost'>true</b:widget-setting>
                  </b:widget-settings>
                  <b:includable id='main'>
                    <!-- Only display title if it's non-empty -->
                    <b:if cond='data:title != &quot;&quot;'>
                      <h3 class='uk-card-title'>
                        <data:title/>
                      </h3>
                    </b:if>
                    <b:include name='content'/>
                  </b:includable>
                  <b:includable id='content'>
                    <dt>
                      <a expr:href='data:postUrl'>
                        <data:postTitle/>
                      </a>
                    </dt>
                    <dd>
                      <data:postSummary/>
                    </dd>
                  </b:includable>
                </b:widget>
              </b:section>
            </div>
            <div>
              <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor.</p>
            </div>
          </div>
          <b:section id='site-info'>
            <b:widget id='BloggerButton1' locked='false' title='' type='BloggerButton' version='1'>
              <b:widget-settings>
                <b:widget-setting name='button'>https://resources.blogblog.com/html/buttons/blogger-powerby-blue.gif</b:widget-setting>
              </b:widget-settings>
              <b:includable id='main'>
                <hr/>
                <a href='https://www.blogger.com'>
                  <img alt='Powered By Blogger' expr:src='data:fullButton'/>
                </a>
              </b:includable>
            </b:widget>
          </b:section>
        </div>
      </footer>
    </div>
    <!--all end-->
    <!--scripts begin-->
    <script src='https://cdn.jsdelivr.net/npm/uikit@3.5.10/dist/js/uikit.min.js'/>
    <script src='https://cdn.jsdelivr.net/npm/uikit@3.5.10/dist/js/uikit-icons.min.js'/>
    <!--scripts end-->
  </body>
</html>
